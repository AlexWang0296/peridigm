	  <ParameterList name="Rythmos Integration Control">
		<Parameter name="Take Variable Steps" type="bool" value="false"/>
		<Parameter name="Fixed dt" type="double" value="1.0"/>
	  </ParameterList>
	</ParameterList>
  </ParameterList>
</ParameterList>
